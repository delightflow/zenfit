{
  "expo": {
    "name": "ZenFit",
    "slug": "zenfit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "scheme": "zenfit",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D0D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zenfit.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D0D0D"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.zenfit.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#4EEEB0"
        }
      ]
    ],
    "experiments": {
      "baseUrl": "/zenfit"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6a2ef220-866f-4ab6-b66f-1b57226f7cdf"
      }
    },
    "owner": "seungyeoljeon",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/6a2ef220-866f-4ab6-b66f-1b57226f7cdf"
    }
  }
}
